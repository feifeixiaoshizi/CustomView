<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:seekbar="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    >

   <com.study.customview.customview.event.EventImageView
       android:id="@+id/image"
       android:src="@drawable/matrix"
       android:layout_width="150dp"
       android:layout_height="100dp" />

<!--

   <attr name="backGroundColor" format="color"/>
   <attr name="backGroundWidth" format="dimension"/>
   <attr name="backGroundHeigh" format="dimension"/>

   <attr name="frontGroundColor" format="color"/>
   <attr name="frontGroundWidth" format="dimension"/>
   <attr name="frontGroundHeigh" format="dimension"/>

   <attr name="circleRadius" format="dimension"/>
-->


   <com.study.customview.customview.test.SeekBarView
       android:layout_marginTop="20dp"

       seekbar:backGroundColor="@color/colorAccent"
       seekbar:backGroundWidth="5dp"

       seekbar:frontGroundColor="#3F51B5"
       seekbar:frontGroundWidth="6dp"

       seekbar:circleRadius="8dp"


       android:layout_width="140dp"
       android:layout_height="140dp" />

</LinearLayout>
